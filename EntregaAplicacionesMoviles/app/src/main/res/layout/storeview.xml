<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="false"
    android:layout_marginTop="20dp"
    android:background="@drawable/custom_backgroundthree">


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/profileImageSV"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp"
        app:layout_constraintEnd_toStartOf="@+id/textView18"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/storeNameSV"
        style="@style/TitleTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="TextView"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/profileImageSV"
        app:layout_constraintStart_toEndOf="@+id/profileImageSV"
        app:layout_constraintTop_toTopOf="@+id/profileImageSV" />

    <Button
        android:id="@+id/seeAllSV"
        style="@style/TertiaryButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#00FFFFFF"
        android:stateListAnimator="@null"
        android:text="Ver todo >"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/productOneImageSV"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileImageSV"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/productOneNameSV"
        style="@style/TitleThree"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/productOneImageSV"
        app:layout_constraintTop_toBottomOf="@+id/productOneImageSV" />

    <TextView
        android:id="@+id/productPriceOneSV"
        style="@style/TextTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="16dp"
        android:text="TextView"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/productOneImageSV"
        app:layout_constraintTop_toBottomOf="@+id/productOneNameSV" />

    <ImageView
        android:id="@+id/productTwoImageSV"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toStartOf="@+id/productThreeImageSV"
        app:layout_constraintStart_toEndOf="@+id/productOneImageSV"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/productTwoNameSV"
        style="@style/TitleThree"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/productTwoImageSV"
        app:layout_constraintTop_toBottomOf="@+id/productTwoImageSV" />

    <TextView
        android:id="@+id/productPriceTwoSV"
        style="@style/TextTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="16dp"
        android:text="TextView"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/productTwoImageSV"
        app:layout_constraintTop_toBottomOf="@+id/productTwoNameSV" />

    <ImageView
        android:id="@+id/productThreeImageSV"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seeAllSV"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/productThreeNameSV"
        style="@style/TitleTwo"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/productThreeImageSV"
        app:layout_constraintTop_toBottomOf="@+id/productThreeImageSV" />

    <TextView
        android:id="@+id/productPriceThreeSV"
        style="@style/TextTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="16dp"
        android:text="TextView"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/productThreeImageSV"
        app:layout_constraintTop_toBottomOf="@+id/productThreeNameSV" />

</androidx.constraintlayout.widget.ConstraintLayout>